{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "branchPrefix": "renovate/react-ts-",
      "matchFileNames": [
        "examples/react-ts/package.json"
      ],
      "commitMessageSuffix": "[react-ts]",
      "addLabels": [
        "Package: react-ts"
      ]
    },
    {
      "branchPrefix": "renovate/vanilla-",
      "matchFileNames": [
        "examples/vanilla/package.json"
      ],
      "commitMessageSuffix": "[vanilla]",
      "addLabels": [
        "Package: vanilla"
      ]
    }
  ]
}
