    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplex-noise": "4.0.1",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@embyth/eslint-plugin": "0.1.8",
    "@embyth/prettier-config": "1.0.0",
    "@embyth/scss-design-system": "workspace:*",
    "@embyth/stylelint-config": "0.3.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.42.0",
    "prettier": "3.0.3",
    "stylelint": "15.10.3",
    "typescript": "5.0.2",
    "vite": "4.4.0",
    "vite-tsconfig-paths": "4.2.0"
  }
