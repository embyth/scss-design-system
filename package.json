{
  "name": "@embyth/scss-design-system",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "My personal Design System built on SCSS",
  "author": "Rostyslav Miniukov <miniukovrostyslav@gmail.com> (https://github.com/embyth)",
  "license": "MIT",
  "homepage": "https://github.com/embyth/scss-design-system#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/embyth/scss-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/embyth/scss-design-system/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./scss": {
      "types": "./dist/scss.d.ts",
      "require": "./dist/scss.cjs",
      "import": "./dist/scss.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=18.15.0",
    "pnpm": ">=8.1.0"
  },
  "scripts": {
    "dev": "vite",
    "clean:cache": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "clean": "pnpm run clean:cache && pnpm run clean:deps",
    "lint": "pnpm exec stylelint \"src/**/*.scss\" --config .stylelintrc",
    "build": "pnpm run clean:cache && unbuild"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@embyth/eslint-plugin": "0.1.8",
    "@embyth/stylelint-config": "0.1.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react": "4.0.0",
    "devmoji": "2.3.0",
    "eslint": "8.43.0",
    "husky": "8.0.3",
    "sass": "1.63.4",
    "stylelint": "15.8.0",
    "typescript": "5.1.3",
    "vite": "4.3.9"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "peerDependencies": {
    "sass": "1.63.4"
  }
}